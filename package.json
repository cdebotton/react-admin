{
  "name": "cdb",
  "version": "1.0.0",
  "description": "debotton.io",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./public/*",
    "localhost": "sleep 1; which open && open http://localhost:3000",
    "browser": "sleep 2; concurrent 'npm run start' 'npm run localhost'",
    "webpack": "webpack --verbose --colors --display-eror-details --config ./webpack/production.config.js",
    "webpack:watch": "NODE_ENV=development node --harmony ./webpack/dev-server.js",
    "prebuild": "npm run clean -s",
    "build": "npm run test -s && npm run webpack -s",
    "serve": "NODE_ENV=production NODE_PATH=\"./app\" supervisor --harmony -e js ./index.js",
    "serve:development": "NODE_ENV=development NODE_PATH=\"./app\" supervisor --harmony -e js ./index.js",
    "watch": "npm run clean -s && concurrent --kill-others 'npm run webpack:watch -s' 'npm run serve:development -s'",
    "start": "NODE_ENV=production npm run build -s && npm run serve -s",
    "test": "mocha --harmony --compilers js:babel/register ./test/**/*Test.js",
    "test:watch": "npm run test -- --watch --watch-extension js",
    "db": "sequelize"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cdebotton/cdb.git"
  },
  "keywords": [
    "assets",
    "library",
    "react"
  ],
  "author": "Christian de Botton <debotton@brooklynunited.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cdebotton/cdb/issues"
  },
  "homepage": "https://github.com/cdebotton/cdb",
