    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.2.16",
    "css-loader": "^0.12.0",
    "del": "^1.1.1",
    "eslint": "^0.20.0",
    "eslint-loader": "^0.11.1",
    "extract-text-webpack-plugin": "^0.7.0",
    "file-loader": "^0.8.1",
    "font-awesome": "^4.3.0",
    "gulp": "^3.8.11",
    "gulp-plumber": "^1.0.0",
    "gulp-spawn-mocha": "^2.0.1",
    "gulp-util": "^3.0.4",
    "jeet": "^6.1.2",
    "json-loader": "^0.5.1",
    "mocha": "^2.2.4",
    "mocha-loader": "^0.7.1",
    "nib": "^1.1.0",
